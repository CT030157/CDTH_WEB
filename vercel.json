{
  "version": 2,
  "builds": [
    {
      "src": "server/products.js",
      "use": "@vercel/node"
    },
    {
      "src": "server/users.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/users/(.*)",
      "dest": "server/users.js"
    },
    {
      "src": "/api/products/(.*)",
      "dest": "server/products.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "server/products.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ]
}
